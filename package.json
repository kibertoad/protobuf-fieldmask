{
  "name": "protobuf-fieldmask",
  "version": "1.2.0",
  "description": "Library for generating and applying FieldMask",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "scripts": {
    "coveralls": "nyc report --reporter=lcov",
    "test": "mocha ./test",
    "test:coverage": "nyc npm test",
    "lint": "npx @biomejs/biome@1.5.3 lint index.js lib test biome.json",
    "lint:fix": "npx @biomejs/biome@1.5.3 check --apply index.js lib test biome.json",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/protobuf-fieldmask.git"
  },
  "keywords": [
    "protocol",
    "buffers",
    "protobuf",
    "field",
    "mask",
    "json",
    "grpc",
    "fieldmask"
  ],
  "homepage": "http://github.com/kibertoad/protobuf-fieldmask",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^2.2.1"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "files": [
    "lib/*",
    "index.js",
    "LICENSE",
    "README.md"
  ]
}
