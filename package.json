{
  "name": "protobuf-fieldmask",
  "version": "1.2.0",
  "description": "Library for generating and applying FieldMask",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "scripts": {
    "coveralls": "nyc report --reporter=lcov",
    "test": "mocha ./test",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/protobuf-fieldmask.git"
  },
  "keywords": [
    "protocol",
    "buffers",
    "protobuf",
    "field",
    "mask",
    "json",
    "grpc",
    "fieldmask"
  ],
  "homepage": "http://github.com/kibertoad/protobuf-fieldmask",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^2.2.1"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "files": [
    "lib/*",
    "index.js",
    "LICENSE",
    "README.md"
  ]
}
